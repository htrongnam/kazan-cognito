{
  "name": "kazan-cognito",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/htrongnam/kazan-cognito.git"
  },
  "author": "NamHoang",
  "license": "MIT",
  "dependencies": {
    "amazon-cognito-identity-js": "^2.0.3",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "morgan": "^1.8.0",
    "node-fetch": "^2.1.2",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "json-loader": "^0.5.7",
    "nodemon": "^1.9.2",
    "webpack": "^4.6.0"
  },
  "bugs": {
    "url": "https://github.com/htrongnam/kazan-cognito/issues"
  },
  "private": true,
  "homepage": "https://github.com/htrongnam/kazan-cognito#readme"
}
